{
	"manifest_version": 2,
	"name": "ももな",
	"version": "1.0",
	"description": "MOMONA IS SO CUTE.",
	"icons": {
	"48": "icons/48.png",
	"96": "icons/96.png"
	},
	"browser_action": {
		"default_icon": "icons/32.png",
		"default_title": "Beastify",
		"default_popup": "popup/choose_beast.html"
	},
	"permissions": [
		"activeTab"
	],
	"web_accessible_resources": [
		"beasts/frog.jpg",
		"beasts/turtle.jpg",
		"beasts/snake.jpg"
	]
}