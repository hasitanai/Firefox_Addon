{
  "manifest_version": 2,

  "name": "ももなマウス",
  "description": "MOMONA IS SO CUTE.",
  "version": "1.00",
  "default_locale": "ja",
  
    "commands": {
    "display-onetab": {
      "suggested_key": {
        "default": "Alt+Shift+1"
      },
      "description": "__MSG_displayOneTab__"
    },
    "send-current-tab-to-onetab": {
      "description": "__MSG_sendCurrentTabToOneTab__"
    }
  },
  
          
  "icons": {
      "96": "icons/96.png",
      "48": "icons/48.png",
   },
  "permissions": [
        "tabs",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking",
    "contextMenus",
    "storage"
  ],
  "browser_action": {
    "default_icon": {
      "48": "icons/48.png",
      "32": "icons/32.png"
    }
  },
  "content_scripts": [
  {
    "matches": ["http://*/*", "https://*/*"],
    "js": ["ext-onetab-concatenated-sources-contentscript.js"]
  }
  ],

  "background": {
    "scripts": ["ext-onetab-concatenated-sources-background.js"]
  }

    


}